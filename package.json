{
  "name": "graphql-error-handler",
  "license": "MIT",
  "version": "0.14.0",
  "main": "dist/index.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.js.map"
  ],
  "peerDependencies": {
    "@types/graphql": "*"
  },
  "devDependencies": {
    "@apollo/client": "^3.5.7",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.38",
    "esbuild": "^0.14.14",
    "esbuild-jest": "^0.5.0",
    "graphql": "^16.2.0",
    "graphql-tag": "^2.12.6",
    "jest": "^27.4.7",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vitest": "^0.2.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/erikpukinskis/graphql-error-handler.git"
  },
  "homepage": "https://github.com/erikpukinskis/graphql-error-handler",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "check:types": "tsc --noEmit",
    "build": "npm run build:vite; npm run build:types",
    "build:vite": "vite build",
    "build:types": "tsc src/index.ts --showConfig --declaration --emitDeclarationOnly --outDir dist --esModuleInterop --skipLibCheck",
    "bump": "./bump.sh"
  },
}
